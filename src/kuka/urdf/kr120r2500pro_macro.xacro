<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <xacro:macro name="kuka_kr120r2500pro" params="prefix">
    <!-- LINKS -->
    <!-- base link -->
    <link name="${prefix}base_link">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/base_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/base_link.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 1 (A1) -->
    <link name="${prefix}link_1">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/link_1.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/link_1.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 2 -->
    <link name="${prefix}link_2">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/link_2.dae"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/link_2.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 3 -->
    <link name="${prefix}link_3">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/link_3.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/link_3.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 4 -->
    <link name="${prefix}link_4">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/link_4.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/link_4.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 5 -->
    <link name="${prefix}link_5">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/link_5.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/link_5.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 6 -->
    <link name="${prefix}link_6">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/visual/link_6.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka/meshes/kr120r2500pro/collision/link_6.stl" />
        </geometry>
      </collision>
    </link>
    <!-- Following REP199, this frame shall be use to attach EEF or other equipment -->
    <link name="${prefix}flange" />
    <!-- END LINKS -->

    <!-- JOINTS -->
    <!-- joint 1 (A1) -->
    <joint name="${prefix}joint_a1" type="revolute">
      <origin xyz="0 0 0.675" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <axis xyz="0 0 -1"/>
      <limit effort="0" lower="${-radians(185)}" upper="${radians(185)}" velocity="${radians(156)}"/>


      <state_interace name="position"/>
      <state_interace name="velocity"/>

    </joint>
    <!-- joint 2 (A2) -->
    <joint name="${prefix}joint_a2" type="revolute">
      <origin xyz="0.35 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-radians(155)}" upper="${radians(35)}" velocity="${radians(156)}"/>



      <state_interace name="position"/>
      <state_interace name="velocity"/>

    </joint>
    <!-- joint 3 (A3) -->
    <joint name="${prefix}joint_a3" type="revolute">
      <origin xyz="1.150 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-radians(130)}" upper="${radians(154)}" velocity="${radians(156)}"/>

      <sensor name="joint_a3_position" type="force_torque">
        <interface>position</interface>
      </sensor>


      <state_interace name="position"/>
      <state_interace name="velocity"/>
    </joint>
    <!-- joint 4 (A4) -->
    <joint name="${prefix}joint_a4" type="revolute">
      <origin xyz="1.0 0 -0.041" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="${-radians(350)}" upper="${radians(350)}" velocity="${radians(330)}"/>

      <state_interace name="position"/>
      <state_interace name="velocity"/>
    </joint>
    <!-- joint 5 (A5) -->
    <joint name="${prefix}joint_a5" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-radians(130)}" upper="${radians(130)}" velocity="${radians(330)}"/>

      <state_interace name="position"/>
      <state_interace name="velocity"/>


    </joint>
    <!-- joint 6 (A6) -->
    <joint name="${prefix}joint_a6" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="${-radians(350)}" upper="${radians(350)}" velocity="${radians(615)}"/>

      <state_interace name="position"/>
      <state_interace name="velocity"/>


    </joint>
    <!-- tool frame - fixed frame -->
    <joint name="${prefix}joint_a6-flange" type="fixed">
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
      <origin xyz="0.215 0 0" rpy="0 0 0"/>
    </joint>
    <!-- END JOINTS -->

    <!-- ROS base_link to KUKA $ROBROOT coordinate system transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <!-- This frame corresponds to the $TOOL coordinate system in KUKA KRC controllers -->
    <link name="${prefix}tool0" />
    <joint name="${prefix}flange-tool0" type="fixed">
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
      <origin xyz="0 0 0" rpy="0 ${radians(90)} 0"/>
    </joint>


    <gazebo>
        <!-- <plugin filename="libgz_ros2_control-system.so" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
          <parameters>$(find kuka)/config/controllers.yaml</parameters>
        </plugin> -->

        <plugin filename="libgz_ros2_control-system.so" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
          <parameters>$(find kuka_moveit)/config/ros2_controllers.yaml</parameters>
          <!-- <robot_param_node>robot_state_publisher</robot_param_node> -->
        </plugin>




        <plugin filename="gz-sim-joint-state-publisher-system"
          name="gz::sim::systems::JointStatePublisher">
          <topic>joint_states</topic> <!--from <ros><remapping> -->

          <joint_name>joint_a1</joint_name>
          <joint_name>joint_a2</joint_name>
          <joint_name>joint_a3</joint_name>
          <joint_name>joint_a4</joint_name>
          <joint_name>joint_a5</joint_name>
          <joint_name>joint_a6</joint_name>
        </plugin>


        <plugin
          filename="gz-sim-joint-trajectory-controller-system"
          name="gz::sim::systems::JointTrajectoryController">
          <topic>/Arm_controller/joint_trajectory</topic>
        </plugin>




    </gazebo>


    <ros2_control name="GazeboSimSystem" type="system">
      <hardware>
        <plugin>mock_components/GenericSystem</plugin>

      </hardware>
        <joint name="joint_a1">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="joint_a2">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="joint_a3">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="joint_a4">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="joint_a5">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="joint_a6">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
    </ros2_control>




  </xacro:macro>
</robot>


